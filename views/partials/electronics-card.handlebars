<section class="card">
  <header class="card-title">{{title}}</header>

  <div class="card-body">
    <img src="/images/{{filename}}" />
    <p class="des">Description: {{description}}</p>
    <p>Sku # {{sku}}</p>
    <p>Model # {{model}}</p>
    <p>Price: {{price}}</p>
    <button class="btn btn-primary" id="shop" @click="addToCart">Add to cart</button>
  </div>
  <script src="./js/cart.js"></script>
</section>